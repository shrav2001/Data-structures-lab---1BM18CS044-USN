#include<stdio.h>
void insert(int item,int *r,int q[],int *f);
int delete(int *r,int q[],int *f);
void display(int q[],int f,int r);
void insert(int item,int *r,int q[],int *f)
{
if(*f==*r+1 || ( *f==0 && *r==99 ) )
{
printf("queue is full\n");
return;
}
if(*f==-1)
{
*f=0;
}
*r=(*r+1)%100;
q[*r]=item;
}
int delete(int *r,int q[],int *f)
{
int item;
if(*f==-1)
{
printf("queue is empty\n");
return -9999;
}
else
{
item=q[*f];
if(*f==*r)
{
*f=-1;
*r=-1;
printf("queue is empty\n");
}
else
*f=(*f+1)%100;
return item;
}
}
void display(int q[],int f,int r)
{
int i;
if(f==-1)
{
printf("queue is empty\n");
return;
}
printf("contents of queue are\n");
for(i=f;i!=r;(i+1)%100)
{
printf("%d",q[i]);
}
printf("%d",q[r]);
}
int main()
{
int q[100],f=0,r=-1,b,ele,i=0,e;
do
{
printf(" Press 1: insert\n Press 2: delete\n Press 3: Display\n Press 4:Exit\n");
scanf("%d",&b);
switch(b)
{ case 1: printf("Enter the element you want to insert :\n");
          scanf("%d",&ele);
          insert(ele,&r,q,&f);
          break;

  case 2: e=delete(&r,q,&f);
          if(e!=9999)
          {
            printf("The deleted element is %d\n",e);
          }
          break;

  case 3: display(q,f,r);
          break;

  case 4: break;
  
  default: printf("you have entered a wrong key\n");
           break;
};
}while(b!=4);

return 0;
}
